{
  "name": "yourNEU",
  "description": "Make NEU not terrible!",
  "version": "1.0",

  "permissions": [
    "http://myneu.neu.edu/*",
    "https://myneu.neu.edu/*",
    "activeTab"
  ],

  "options_page": "options.html",

  
  "content_scripts": [
    {
      "matches": [
        "https://myneu.neu.edu/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],

  "manifest_version": 2
}
