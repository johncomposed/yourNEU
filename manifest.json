{
  "name": "yourNEU",
  "description": "Make NEU not terrible!",
  "version": "1.0",

  "permissions": [
    "http://myneu.neu.edu/*",
    "https://myneu.neu.edu/*",
    "activeTab",
    "tabs"
  ],
  
  "content_scripts": [
    {
      "matches": [
        "https://myneu.neu.edu/*",
        "http://myneu.neu.edu/*"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_end"
    }
  ],

  "manifest_version": 2
}
